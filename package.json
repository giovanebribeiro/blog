    "gatsby-plugin-netlify": "^3.8.0",
    "gatsby-plugin-offline": "^4.8.0",
    "gatsby-plugin-sitemap": "^4.4.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.24",
    "gatsby-recipes": "^0.19.0",
    "gatsby-source-filesystem": "^3.8.0",
